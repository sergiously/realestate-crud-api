                        }
                      ],
                      "total":39
                    }
                  },
                  "Example successful no-results response":{
                    "value":{
                      "results":[
                        
                      ],
                      "total":0
                    }
                  }
                }
              }
            }
          },
          "400":{
            "summary":"Invalid input query params",
            "description":"Invalid input query params response",
            "content":{
              "application/json":{
                "schema":{
                  "$ref":"#/components/schemas/GenericResponse"
                },
                "example":{
                  "value":{
                    "message":"Invalid request"
                  }
                }
              }
            }
          },
          "401":{
            "summary":"Unauthorized response",
            "description":"Unauthorized response",
            "content":{
              "application/json":{
                "schema":{
                  "$ref":"#/components/schemas/GenericResponse"
                },
                "example":{
                  "value":{
                    "message":"Unauthorized"
                  }
                }
              }
            }
          },
          "default":{
            "summary":"Unexpected error",
            "description":"Unexpected error",
            "content":{
              "application/json":{
                "schema":{
                  "$ref":"#/components/schemas/Error"
                },
                "example":{
                  "value":{
                    "message":"Internal server error"
                  }
                }
              }
            }
          }
        }
      },

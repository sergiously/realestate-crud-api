{
  "name": "ts-postgresql-local",
  "version": "1.0.0",
  "description": "Real estate CRUD REST API - Based on Express, Sequelize, and PostgreSQL",
  "main": "dist/app.js",
  "engines": {
    "node": "^20"
  },
  "scripts": {
    "dev": "nodemon --inspect=0.0.0.0:9229 --signal SIGINT --nolazy ./src/app.ts",
    "start": "node ./dist/app.js",
    "build": "node build.js",
    "lint": "tslint -p tsconfig.json && prettier --write \"src/**/*.ts\"",
    "test": "jest --silent false --config jest.config.js --coverage --reporter=clover"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/sergiously-templates/realestate-api-crud/ts-postgresql-local.git"
  },
  "bugs": {
    "url": "https://gitlab.com/sergiously-templates/realestate-api-crud/ts-postgresql-local/issues"
  },
  "homepage": "https://gitlab.com/sergiously-templates/realestate-api-crud/ts-postgresql-local#readme",
  "files": [
    "dist/*"
  ],
  "keywords": [
    "docker",
    "docker-compose",
    "express",
    "sequelize",
    "postgresql",
    "api",
    "crud",
    "typescript"
  ],
  "devDependencies": {
    "@types/config": "^3.3.4",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/joi": "^17.2.3",
    "@types/node": "^20.14.10",
    "@types/swagger-ui-express": "^4.1.6",
    "concurrently": "^8.2.2",
    "esbuild": "^0.23.0",
    "esbuild-plugin-copy": "^2.1.1",
    "esbuild-plugin-tsc": "^0.4.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.2",
    "pretty": "^2.0.0",
    "ts-jest": "^29.2.0",
    "ts-node": "^10.9.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "@joi/date": "^2.1.1",
    "bcryptjs": "^2.4.3",
    "config": "^3.3.12",
    "express": "^4.19.2",
    "express-joi-validation": "^5.0.1",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.12.0",
    "redis": "^4.6.15",
    "sequelize": "^6.37.3",
    "swagger-ui-express": "^5.0.1"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "execMap": {
      "ts": "node --require ts-node/register"
    }
  },
  "author": "Sergio N. Raggio",
  "license": "GPL-3.0-or-later"
}
